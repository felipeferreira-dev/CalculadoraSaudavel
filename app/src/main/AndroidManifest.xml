<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.dev.felipeferreira.calculadorasaudavel2">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_fit_calc"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_fit_calc"
        android:supportsRtl="true"
        android:theme="@style/Theme.CalculadoraSaudavel2">

        <activity android:name=".ListCalcActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity android:name=".TmbActivity" />
        <activity android:name=".ImcActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>